{
  "name": "@vpcs/vpcs-nextjs",
  "resolve": { "fallback": { "fs": false, "path": false } },
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/vpcs/core/vpcs-nextjs/src",
  "projectType": "library",
  "publishConfig": {
    "registry": "https://npm.groupondev.com"
  },
  "importPath": "@vpcs/vpcs-nextjs",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": ["platform vpcs"],
  "targets": {
    "lint": {
      "executor": "@nx/node:eslint",
      "options": {
        "lintFilePatterns": ["libs/vpcs/core/vpcs-nextjs/**/*.ts"]
      }
    },
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/vpcs/core/vpcs-nextjs",
        "main": "libs/vpcs/core/vpcs-nextjs/src/index.js",
        "tsConfig": "libs/vpcs/core/vpcs-nextjs/tsconfig.lib.json",
        "assets": ["libs/vpcs/core/vpcs-nextjs/*.md"],
        "generatePackageJson": true,
        "format": ["cjs"]
      }
    },
    "nx-release-publish": {
      "executor": "@nx:js/release-publish",
      "options": {
        "packageRoot": "dist/{projectRoot}",
        "registry": "https://npm.groupondev.com",
        "access": "public",
        "dryRun": false
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/vpcs/core/vpcs-nextjs/jest.config.js"
      }
    }
  }
}
