{
  "name": "@vpcs/grpn-suboptimal",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/vpcs/core/grpn-suboptimal/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": ["platform vpcs"],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/vpcs/core/grpn-suboptimal",
        "main": "libs/vpcs/core/grpn-suboptimal/src/index.js",
        "tsConfig": "libs/vpcs/core/grpn-suboptimal/tsconfig.lib.json",
        "assets": ["libs/vpcs/core/grpn-suboptimal/*.md"],
        "generatePackageJson": true,
        "format": ["cjs"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
}
